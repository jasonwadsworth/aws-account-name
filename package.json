{
  "name": "aws-account-name-display",
  "version": "1.0.0",
  "description": "Chrome extension to display AWS account names in the console",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "fast-check": "^3.15.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": ["<rootDir>/tests"],
    "testMatch": ["**/*.test.js", "**/*.property.test.js"],
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
  }
}
